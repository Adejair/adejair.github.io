.button-container
  text-align center
  margin-top 10px
  margin-left 8px

.button
  border 2px solid white
  border-radius 35px

  margin-right 8px

  transition all 1.0s
  &:hover
    border-color grey

a.button
  display inline-block
  padding 3px 14px

  //width 60px
  //height 30px

  font-family 'Open Sans', Arial
  font-size 20px

  color white
  text-decoration none
